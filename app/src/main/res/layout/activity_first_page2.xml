<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/city_background"
    tools:context=".first_page">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="94dp"
        android:layout_height="103dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:srcCompat="@drawable/profile" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="94dp"
        android:layout_height="103dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="216dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:srcCompat="@drawable/profile" />

    <TextView
        android:id="@+id/Browsertxt"
        android:layout_width="271dp"
        android:layout_height="53dp"
        android:fontFamily="@font/orbitron_medium"
        android:text="SCHOOL STAFF BROWSER"
        android:textAlignment="center"
        android:textColor="#00bfff"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.028" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="94dp"
        android:layout_height="103dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="344dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.279"
        app:srcCompat="@drawable/profile" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/orbitron_medium"
        android:text="Teacher 1"
        android:textAlignment="center"
        android:textColor="#9ac4ff"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.036"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/orbitron_medium"
        android:text="Teacher 2"
        android:textAlignment="center"
        android:textColor="#9ac4ff"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.387"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="132dp"
        android:fontFamily="@font/orbitron_medium"
        android:text="Teacher 3"
        android:textAlignment="center"
        android:textColor="#9ac4ff"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.397"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/orbitron_medium"
        android:text="LIST OF INSTRUCTORS"
        android:textColor="#00bfff"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.108"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Browsertxt" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="285dp"
        android:layout_height="63dp"
        android:layout_marginTop="8dp"
        android:text="Also known as MR. COOL DUDE, He can be found at faculty room at 2nd floor."
        android:textAlignment="center"
        android:textColor="#9ac4ff"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.437"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="285dp"
        android:layout_height="63dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="216dp"
        android:text="Also known as MS. CUTIE PIE, He can be found at Comlab at 4th floor."
        android:textAlignment="center"
        android:textColor="#9ac4ff"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView2" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="285dp"
        android:layout_height="63dp"
        android:layout_marginTop="12dp"
        android:text="Also known as MRS. NICE, He can be found at Guidance office at 2nd floor ADMIN building."
        android:textAlignment="center"
        android:textColor="#9ac4ff"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.437"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/Availbtn"
        android:layout_width="131dp"
        android:layout_height="41dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#00bfff"
        android:fontFamily="@font/orbitron_medium"
        android:text="AVAILABILITY"
        android:textColor="#000000"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Inboxbtn"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/Inboxbtn"
        android:layout_width="93dp"
        android:layout_height="45dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#00bfff"
        android:fontFamily="@font/orbitron_medium"
        android:text="INBOX"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Updbtn"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Availbtn" />

    <Button
        android:id="@+id/Updbtn"
        android:layout_width="162dp"
        android:layout_height="49dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#00bfff"
        android:fontFamily="@font/orbitron_medium"
        android:text="DEV'S UPDATES"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Inboxbtn" />
</androidx.constraintlayout.widget.ConstraintLayout>